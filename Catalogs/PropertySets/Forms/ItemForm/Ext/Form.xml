			</Type>
			<UseAlways>
				<Field>PropertyTree.InformationIconNumber</Field>
				<Field>~PropertyTree.НомерКартинкиСведения</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Table.Ref,
	CASE
		WHEN Table.Ref IN (&amp;arraySelected)
			THEN TRUE
		ELSE FALSE
	END AS Selected,
	CASE
		WHEN Table.IsFolder
			THEN 0
		WHEN Table.IsAdditionalInformation
			THEN 2
		ELSE 1
	END + CASE
		WHEN Table.DeletionMark
			THEN 3
		ELSE 0
	END AS InformationIconNumber,
	Table.IsFolder
FROM
	ChartOfCharacteristicTypes.AdditionalPropertiesAndInformation AS Table</QueryText>
